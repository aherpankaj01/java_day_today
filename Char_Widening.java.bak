class  Char_Widening
{
	public static void main(String[] args) 
	{
		double b1= 'c';  //10000.0
		System.out.println(b1);
		
		float b2= 'c';  //10000.0
		System.out.println(b2);
		
		long b3= 'c';  //10000
		System.out.println(b3);
		
		int b4= 'c';  //10000
		System.out.println(b4);
		
	}
}